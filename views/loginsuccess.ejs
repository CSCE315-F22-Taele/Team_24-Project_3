<div class="row mt-5" id = "1">
    <div class="col-md-6 m-auto">
      <div class="card card-body text-center">
        <script src="https://translate.google.com/translate_a/element.js?cb=translateDropdown"></script>
        <script>
          function translateDropdown() {
            new google.translate.TranslateElement(
              {pageLanguage: 'en'}, 'google_translate_id'
            )
          }
        </script>
        <div id="google_translate_id"></div>
        <h1>Log in successfully!</h1>
        <P>
         <strong class = "text-primary">Your ID</strong> : <%= user.id %> <br>
         <strong class = "text-primary">Your Email</strong>: <%= user.emails[0].value %><br>
         <strong class = "text-primary">Your Name</strong>: <%= user.displayName %>
        </P>
        
        <a href="managers/managers" class="btn btn-primary btn-block mb-2"
        >Managers</a>
        <a href="servers/servers" class="btn btn-primary btn-block mb-2"
        >Servers</a>
</div>
</div>
</div>