<div class="row mt-5">
    <div class="col-md-6 m-auto">
      <div class="card card-body text-center">

<table>
    <tr>
       <th>id</th><th>item</th><th>price</th>
    </tr>
 
    <% for (var i = 0; i < menu.length; i++) { %>
     <tr>
       <td><%= menu[i].id %></td>
       <td><%= menu[i].item %></td>
       <td><%= menu[i].price %></td>
     </tr>    
    <% } %>
 </table>